@model List<string>

@{ 
    string dato = DateTime.Now.ToLongDateString();
    string cl = "red";
    string cl2 = "blue";

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h3 style="Background-color : @cl">@dato</h3>
    @if (DateTime.Now.Millisecond % 2 == 0)
    {
        <h3 style="color: @cl2">Datoen er @dato</h3>
    }
    else
    {
        <h3 style="color: @cl">Datoen er @dato</h3>
    }



    <h3>Datoen er @dato</h3>


    @for (int i = 0; i < 5; i++)
    {
        <h5>Test</h5>
    }


    @{
        int t = 0;
    }
    <ol>
        @foreach (var item in Model)
        {
            if (++t % 2 == 0)
            {
                <li style="color:red">@item</li>
            }
            else
            {
                <li style="color:green">@item</li>
            }

        }
    </ol>

    <table>
        @for (int i = 0; i < 8; i++)
        {
            <tr>
                @for (int x = 0; x < 8; x++)
                {
                    <td>*</td>
                }
            </tr>
        }
    </table>
</body>
</html>
